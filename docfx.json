{
  "metadata": [
    {
      "src": [
        {
          "files": ["src/**.csproj"]
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      
    }
  ],
  "build": {
    "globalMetadata": {
      "_enableSearch": true
    },
    "content": [
      {
        "files": [
          "MarkdownTest/toc.yml",
          "MarkdownTest/**.md",
          "Who/toc.yml",
          "Who/**.md",
          "toc.yml",
          "**.md"
        ]
      }
    ],
    "resource": [
      {
        "files": ["images/**", "main/images/**"]
      }
    ],
    "overwrite": [
      {
        "files": ["apidoc/**.md"],
        "exclude": ["obj/**", "_site/**"]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": ["default"],
    "postProcessors": [],
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}